{
  "name": "rxjs-shell",
  "version": "1.0.4",
  "description": "rxjs operators for execute shell command with ease",
  "keywords": [
    "rxjs",
    "operator",
    "shell"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/johnny-mh/rxjs-shell.git"
  },
  "license": "MIT",
  "author": "johnny-mh <romz1212@gmail.com>",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.build.json && cp package.json dist/ && cp README.md dist/",
    "cover": "rm -rf .nyc_output && rm -rf coverage && nyc mocha -r ts-node/register -r source-map-support/register test/**/*.spec.ts",
    "lint": "tslint -p tsconfig.json --fix",
    "test": "mocha -r ts-node/register test/**/*.spec.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tslint -p tsconfig.json"
    }
  },
  "nyc": {
    "all": true,
    "exclude": [
      "dist/",
      "coverage",
      "src/index.ts",
      "**/*.spec.ts",
      "**/flycheck**",
      "test/fixtures/**"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "text",
      "html"
    ]
  },
  "dependencies": {
    "rxjs": "^6.0.0",
    "tree-kill": "^1.2.2"
  },
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/chai-spies": "^1.0.0",
    "@types/mkdirp": "^0.5.2",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.9.4",
    "@types/rimraf": "^2.0.2",
    "@types/sinon": "^7.5.2",
    "chai": "*",
    "chai-spies": "^1.0.0",
    "husky": "^1.0.0-rc.14",
    "mkdirp": "^1.0.3",
    "mocha": "^6.2.3",
    "nyc": "^15.0.0",
    "prettier": "^1.14.2",
    "rimraf": "^2.6.2",
    "rxjs": "^6.0.0",
    "sinon": "^9.0.1",
    "source-map-support": "^0.5.9",
    "ts-node": "^8.8.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^3.0.3"
  }
}
