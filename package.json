{
  "name": "rxjs-shell-operators",
  "version": "0.0.1",
  "description": "rxjs operators for execute shell command with ease",
  "main": "dist/index.js",
  "scripts": {
    "lint": "tslint -p tsconfig.json --fix",
    "test": "mocha -r ts-node/register test/**/*.spec.ts",
    "cover": "nyc mocha -r ts-node/register -r source-map-support/register test/**/*.spec.ts"
  },
  "keywords": [
    "rxjs",
    "operator",
    "shell"
  ],
  "author": "johnny-mh <everedifice@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/mkdirp": "^0.5.2",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.9.4",
    "@types/rimraf": "^2.0.2",
    "chai": "*",
    "husky": "^1.0.0-rc.14",
    "mkdirp": "^0.5.1",
    "mocha": "*",
    "nyc": "^13.0.1",
    "prettier": "^1.14.2",
    "rimraf": "^2.6.2",
    "rxjs": "^6.0.0",
    "source-map-support": "^0.5.9",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^3.0.3"
  },
  "peerDependencies": {
    "rxjs": "^6.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tslint -p tsconfig.json --fix"
    }
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "index.ts",
      "coverage",
      "**/*.spec.ts",
      "dist/",
      "**flycheck**"
    ],
    "reporter": [
      "text",
      "html"
    ],
    "all": true
  }
}
